#include <stdio.h>
/*
Add `int max_of_four(int a, int b, int c, int d)` here.
*/
void max_of_four(int a, int b, int c, int d)
{
    if(a >= b && a >= c && a >= d)
    printf("%d",a);
    
    if(b >= c && b >= d && b >= a)
    printf("%d",b);
    
    if(c >= b && c >= a && c >= d)
    printf("%d",c);
    
    if(d >= b && d >= c && d >= a)
    printf("%d",d);
    
    
}
int main() {
    int a, b, c, d;
    scanf("%d %d %d %d", &a, &b, &c, &d);
     max_of_four(a, b, c, d);
    
    return 0;
}
